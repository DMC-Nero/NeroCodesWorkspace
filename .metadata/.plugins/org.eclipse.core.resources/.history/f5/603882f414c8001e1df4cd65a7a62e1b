package qedge_class_examples;

public class Perfect_number {

	
					//6 is a perfect number because 1 + 2 + 3 = 6.
	
	public static void main(String[] args) {
		int i=6;
		while(i>0) {
			int a=1;
			while(a==i) {
				if(i%a==0)
				System.out.println(a+"is a factor");
				
				a++;
			}
			
			
			
			
		}
			
	}

}
