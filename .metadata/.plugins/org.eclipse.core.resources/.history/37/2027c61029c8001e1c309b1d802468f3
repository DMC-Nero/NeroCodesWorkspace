package qedge_class_examples;

public class Armstrong_number {
	//153, it's an Armstrong number because 1^3 + 5^3 + 3^3 equals 153.
	public static void main(String[] args) {
		int a=15334235;
		int t=1;
		while(t<=a) {
			int b=a%10;
			a=a/10;
			
			t++;
		}
		System.out.println(t);
		
		
		
	}

}
