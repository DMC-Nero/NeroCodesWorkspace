package projects;

import java.util.Scanner;

public class Exam {

	public static void main(String[] args) {
		System.out.println("main start");
		
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter a Number");
		int num =sc.nextInt();
		if(num<0) {
			return;
		}
		System.out.println("main end");
		sc.close();
	}

}
